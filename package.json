{
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "NODE_ENV=production tsup",
    "start": "NODE_ENV=production node dist/index.mjs",
    "type-check": "tsc --noEmit",
    "client": "tsx watch src/client.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "import-system": "NODE_ENV=production tsup && node dist/scripts/import/import-system.mjs",
    "reset-db": "NODE_ENV=production tsup && node dist/scripts/db/reset-db.mjs"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "@msgpack/msgpack": "^3.1.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "hono": "^4.10.6",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-sort-json": "^4.1.1",
    "ws": "^8.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@faker-js/faker": "^10.1.0",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/ws": "^8.18.1",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "tsup": "^8.5.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vitest": "^4.0.10"
  },
  "packageManager": "pnpm@10.22.0"
}
